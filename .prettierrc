{
  "semi": true,
  "trailingComma": "es5",
  "singleQuote": true,
  "tabWidth": 4,
  "useTabs": false,
  "printWidth": 100,
  "endOfLine": "lf",
  "bracketSpacing": true,
  "jsxSingleQuote": false,
  "arrowParens": "always",
  "plugins": ["prettier-plugin-tailwindcss"],
  "pluginSearchDirs": false,
  "tailwindConfig": "./tailwind.config.js",
  "tailwindFunctions": ["clsx", "tw", "cn"],
  "overrides": [
      {
          "files": "*.md",
          "options": {
              "tabWidth": 2
          }
      },
      {
          "files": "*.json",
          "options": {
              "tabWidth": 2
          }
      },
      {
          "files": "*.yml",
          "options": {
              "tabWidth": 2
          }
      }
  ]
}